{
  "name": "graphql-pokemon-ui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9000/.netlify/functions/api",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "formik": "^2.1.4",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "yup": "^0.29.1"
  },
  "scripts": {
    "start:mock": "nodemon --watch server/data --exec json-server --p 3001 server/mockserver.js --routes server/routes.json",
    "start:react": "react-scripts start",
    "start": "env-cmd -f .env.development npm-run-all --parallel start:react",
    "mock": "env-cmd -f .env npm-run-all --parallel start:mock start:react",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:ci": "npm run test -- --watchAll=false",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:coveralls": "CI=true COVERALLS_PARALLEL=true npm run test:coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:ci"
    }
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "^1.0.4",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "json-server": "^0.16.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  }
}
